{
    "Union Find": {
        "prefix": "dsu",
        "body": [
            "class DSU:",
            "    def __init__(self, n):",
            "        self.parent = list(range(n))",
            "        self.rank = [0] * n",
            "",
            "    def find(self, i):",
            "        if self.parent[i] != i:",
            "            self.parent[i] = self.find(self.parent[i])",
            "        return self.parent[i]",
            "",
            "    def union(self, i, j):",
            "        root_i = self.find(i)",
            "        root_j = self.find(j)",
            "        if root_i != root_j:",
            "            if self.rank[root_i] > self.rank[root_j]:",
            "                self.parent[root_j] = root_i",
            "            else:",
            "                self.parent[root_i] = root_j",
            "                if self.rank[root_i] == self.rank[root_j]:",
            "                    self.rank[root_j] += 1",
            "            return True",
            "        return False"
        ],
        "description": "Disjoint Set Union"
    },
    "Fenwick Tree": {
        "prefix": "bit",
        "body": [
            "class FenwickTree:",
            "    def __init__(self, size):",
            "        self.tree = [0] * (size + 1)",
            "",
            "    def update(self, i, delta):",
            "        while i < len(self.tree):",
            "            self.tree[i] += delta",
            "            i += i & (-i)",
            "",
            "    def query(self, i):",
            "        s = 0",
            "        while i > 0:",
            "            s += self.tree[i]",
            "            i -= i & (-i)",
            "        return s"
        ],
        "description": "Binary Indexed Tree"
    }
}
