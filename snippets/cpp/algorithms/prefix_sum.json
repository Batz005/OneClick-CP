    "body": [
      "vector<int> prefixXor(n);",
      "prefixXor[0] = a[0];",
      "for (int i = 1; i < n; ++i) {",
      "    prefixXor[i] = prefixXor[i - 1] ^ a[i];",
      "}"
    ]
  },
  "Range XOR Query": {
