[
  {
    "prefix": "cpstkinit",
    "description": "Initialize a stack",
    "category": "stack",
    "body": [
      "stack<${1:int}> ${2:st};"
    ]
  },
  {
    "prefix": "cpstkpq",
    "description": "Push and pop from stack",
    "category": "stack",
    "body": [
      "${1:st}.push(${2:x});",
      "${1:st}.pop();"
    ]
  },
  {
    "prefix": "cpstktop",
    "description": "Access top element of stack",
    "category": "stack",
    "body": [
      "${1:val} = ${2:st}.top();"
    ]
  },
  {
    "prefix": "cpstknempty",
    "description": "Check if stack is not empty",
    "category": "stack",
    "body": [
      "if (!${1:st}.empty()) {",
      "    $0",
      "}"
    ]
  },
  {
    "prefix": "cpqueinit",
