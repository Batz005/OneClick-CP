              "output.txt": ""
            },
            "Fast_IO": {
              "main.cpp": "#include <bits/stdc++.h>\nusing namespace std;\n\nint main() {\n    ios_base::sync_with_stdio(false);\n    cin.tie(NULL);\n    return 0;\n}\n",
              "Main.java": "import java.util.*;\nimport java.io.*;\n\npublic class Main {\n    static class FastReader {\n        BufferedReader br;\n        StringTokenizer st;\n        public FastReader() {\n            br = new BufferedReader(new InputStreamReader(System.in));\n        }\n        String next() {\n            while (st == null || !st.hasMoreElements()) {\n                try {\n                    st = new StringTokenizer(br.readLine());\n                } catch (IOException e) {\n                    e.printStackTrace();\n                }\n            }\n            return st.nextToken();\n        }\n        int nextInt() { return Integer.parseInt(next()); }\n        long nextLong() { return Long.parseLong(next()); }\n        double nextDouble() { return Double.parseDouble(next()); }\n        String nextLine() {\n            String str = \"\";\n            try {\n                str = br.readLine();\n            } catch (IOException e) {\n                e.printStackTrace();\n            }\n            return str;\n        }\n    }\n    public static void main(String[] args) {\n        FastReader sc = new FastReader();\n    }\n}\n",
              "main.py": "import sys\ninput = sys.stdin.readline\n\ndef main():\n    pass\n\nif __name__ == \"__main__\":\n    main()\n",
              "main.js": "const fs = require('fs');\nconst read = fs.readFileSync(0, 'utf-8').trim().split('\\n');\nlet idx = 0;\nconst readline = () => read[idx++];\n\nfunction main() {\n}\n\nmain();\n",
              "input.txt": "",
              "output.txt": ""
            },
            "Debug_Template": {
              "main.cpp": "#include <bits/stdc++.h>\nusing namespace std;\n\n#ifdef LOCAL\n#define debug(x) cerr << #x << \" = \" << x << endl;\n#else\n#define debug(x)\n#endif\n\nint main() {\n    return 0;\n}\n",
              "Main.java": "import java.util.*;\nimport java.io.*;\n\npublic class Main {\n    public static void main(String[] args) {\n        // Debugging\n    }\n}\n",
              "main.py": "import sys\n\ndef debug(*args):\n    print(*args, file=sys.stderr)\n\ndef main():\n    pass\n\nif __name__ == \"__main__\":\n    main()\n",
              "main.js": "const fs = require('fs');\n\nfunction debug(...args) {\n    console.error(...args);\n}\n\nfunction main() {\n}\n\nmain();\n",
              "input.txt": "",
              "output.txt": ""
            },
            "CP_Template": {
              "main.cpp": "#include<bits/stdc++.h>\n// #define ONLINE_JUDGE\nusing namespace std;\n\nint main() {\n\n#ifndef ONLINE_JUDGE\nfreopen(\"input.txt\", \"r\", stdin);\nfreopen(\"output.txt\", \"w\", stdout);\n#endif\n\nint t;\ncin >> t;\nwhile(t--){\n\t\n}\nreturn 0;\n}",
              "Main.java": "import java.util.*;\nimport java.io.*;\n\npublic class Main {\n    public static void main(String[] args) {\n        try {\n            System.setIn(new FileInputStream(\"input.txt\"));\n            System.setOut(new PrintStream(new FileOutputStream(\"output.txt\")));\n        } catch (Exception e) {}\n        Scanner sc = new Scanner(System.in);\n        int t = sc.nextInt();\n        while (t-- > 0) {\n        }\n    }\n}\n",
              "main.py": "import sys\n\n# sys.stdin = open('input.txt', 'r')\n# sys.stdout = open('output.txt', 'w')\n\ndef main():\n    try:\n        t = int(input())\n    except: t = 1\n    for _ in range(t):\n        pass\n\nif __name__ == \"__main__\":\n    main()\n",
              "main.js": "const fs = require('fs');\n// Redirect input/output if needed\n\nfunction main() {\n    // Code\n}\n\nmain();\n",
              "input.txt": "",
              "output.txt": ""
            }
